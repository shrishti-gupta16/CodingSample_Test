    <dimen name="margin_parent">8dp</dimen>
    <dimen name="match_constraints">0dp</dimen>
    <dimen name="margin_extra">12dp</dimen>
    <dimen name="small_margin">4dp</dimen>
    <dimen name="text_size_title">24sp</dimen>
</resources>
